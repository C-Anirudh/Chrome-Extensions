{
    "manifest_version": 2,
    "name": "Budget Manager",
    "description": "This extension tracks your overall spendings",
    "version": "1.0",
    "icons":{
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon16.png"
    },
    
    "options_page": "options.html",
    
    "permissions": [
        "storage",
        "notifications"
    ]

}